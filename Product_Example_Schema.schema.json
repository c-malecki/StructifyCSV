{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Product Example Schema",
    "description": "Just a test schema to build out the editor UI with.",
    "type": "object",
    "properties": {
        "bids": {
            "properties": {
                "initial_price": {
                    "minimum": 5,
                    "type": "number"
                },
                "last_bid": {
                    "type": "null"
                },
                "total_bids": {
                    "type": "integer"
                }
            },
            "type": "object"
        },
        "description": {
            "type": "string"
        },
        "featured": {
            "type": "boolean"
        },
        "images": {
            "items": {
                "type": "string"
            },
            "type": "array"
        },
        "name": {
            "type": "string"
        },
        "seller": {
            "properties": {
                "address": {
                    "properties": {
                        "city": {
                            "type": "string"
                        },
                        "country": {
                            "type": "string"
                        },
                        "line_one": {
                            "type": "string"
                        },
                        "line_two": {
                            "type": "string"
                        },
                        "postal_code": {
                            "type": "string"
                        },
                        "state": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "id": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                }
            },
            "type": "object"
        }
    }
}