{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Product Example Schema",
    "description": "Just a test schema to build out the editor UI with.",
    "type": "object",
    "properties": {
        "bids": {
            "properties": {
                "initialPrice": {
                    "maximum": 99.99,
                    "minimum": 5,
                    "type": "number"
                },
                "lastBid": {
                    "type": "null"
                },
                "totalBids": {
                    "minimum": 0,
                    "type": "integer"
                }
            },
            "required": [
                "initialPrice",
                "lastBid",
                "totalBids"
            ],
            "type": "object"
        },
        "description": {
            "type": "string"
        },
        "featured": {
            "type": "boolean"
        },
        "images": {
            "items": {
                "type": "string"
            },
            "maxItems": 3,
            "minItems": 0,
            "type": "array"
        },
        "name": {
            "minLength": 3,
            "type": "string"
        },
        "seller": {
            "properties": {
                "address": {
                    "properties": {
                        "city": {
                            "type": "string"
                        },
                        "country": {
                            "type": "string"
                        },
                        "lineOne": {
                            "type": "string"
                        },
                        "lineTwo": {
                            "type": "string"
                        },
                        "postalCode": {
                            "type": "string"
                        },
                        "state": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "id": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "test": {
            "type": "array"
        }
    }
}